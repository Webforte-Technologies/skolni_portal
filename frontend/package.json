{
  "name": "eduai-asistent-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:simple": "tsc && vite build --config vite.config.simple.ts",
    "test": "playwright test",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preview:build": "npm run build && vite preview",
    "type-check": "tsc --noEmit",
    "test:e2e": "dotenv -e env.test -- playwright test",
    "test:e2e:parallel": "dotenv -e env.test -- playwright test --config playwright.parallel.config.ts",
    "test:e2e:auth": "dotenv -e env.test -- playwright test tests/auth-core.spec.ts",
    "test:e2e:dashboard": "dotenv -e env.test -- playwright test tests/dashboard-core.spec.ts",
    "test:e2e:ai-generator": "dotenv -e env.test -- playwright test tests/ai-generator-realistic.spec.ts",
    "test:update-snapshots": "dotenv -e env.test -- playwright test --update-snapshots",
    "test:responsive": "jest --config jest.responsive.config.js",
    "test:responsive:watch": "jest --config jest.responsive.config.js --watch",
    "test:responsive:coverage": "jest --config jest.responsive.config.js --coverage",
    "test:visual": "dotenv -e env.test -- playwright test --config playwright.responsive.config.ts",
    "test:visual:update": "dotenv -e env.test -- playwright test --config playwright.responsive.config.ts --update-snapshots",
    "test:visual:ui": "dotenv -e env.test -- playwright test --config playwright.responsive.config.ts --ui"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@types/katex": "^0.16.7",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "docx": "^9.5.1",
    "framer-motion": "^12.23.12",
    "html2canvas": "^1.4.1",
    "idb": "^8.0.3",
    "jspdf": "^3.0.1",
    "katex": "^0.16.22",
    "lucide-react": "^0.292.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.47.0",
    "react-markdown": "^10.1.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.18.0",
    "react-virtuoso": "^4.13.0",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.46.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^5.0.1",
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-image-snapshot": "^6.2.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^7.1.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
